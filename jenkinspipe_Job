pipeline {
    agent any
    tools{
    maven 'Maven_Home'
    jdk 'Home_JAVA'
    }
    stages{
        stage('Initialize') {  
             steps{
                git branch: 'master' url: 'https://github.com/vikramsingh-96/maven-project.git'
                  }
        }
        stage('just trying'){
             steps('Message')
             {
                echo 'This is just a sample build trigger JOB'
             }
            }
        stage("build trigger") {
            steps{
                echo'just a trigger buildingggggggg'
            }
            }
        stage('Build'){
            steps{
            sh 'mvn -B package'
                }

        }
    }
}
